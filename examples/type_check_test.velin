struct User {
    id: string,
    name: string,
    email: string,
}

@GET("/api/users")
fn getUsers(): List<User> {
    let users = db.findAll(User);
    return users;
}

@GET("/api/users/:id")
fn getUser(id: string): User {
    let user = db.find(User, id);
    return user;
}

fn add(a: number, b: number): number {
    return a + b;
}

fn greet(name: string): string {
    return "Hello, " + name;
}
